{
  "name": "mycontracts-frontend",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:production": "VITE_API_URL=https://your-backend-url.com vite build",
    "build:staging": "VITE_API_URL=https://staging-backend-url.com vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "playwright test",
    "test:ui:headed": "playwright test --headed",
    "test:ui:debug": "playwright test --debug",
    "cap:sync": "cap sync",
    "cap:sync:dev": "CAPACITOR_SERVER_URL=http://10.0.2.2:8080 cap sync android",
    "cap:build:android": "npm run build && cap sync android && ./android/gradlew -p android assembleRelease",
    "cap:build:android:dev": "npm run build && npm run cap:sync:dev && ./android/gradlew -p android assembleDebug",
    "cap:build:android:production": "npm run build:production && cap sync android && ./android/gradlew -p android assembleRelease",
    "cap:open:android": "cap open android"
  },
  "dependencies": {
    "@capacitor/android": "^8.0.2",
    "@capacitor/app": "^8.0.1",
    "@capacitor/camera": "^8.0.1",
    "@capacitor/cli": "^8.0.2",
    "@capacitor/core": "^8.0.2",
    "@capacitor/filesystem": "^8.1.1",
    "@capacitor/share": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.30.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^5.1.3",
    "happy-dom": "^20.5.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vitest": "^4.0.18"
  }
}
